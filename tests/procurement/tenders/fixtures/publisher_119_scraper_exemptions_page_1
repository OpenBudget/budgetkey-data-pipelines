<script type="text/javascript" language="javascript">
    function ClearText() {

   var sSearchFreeText = "חפש";
   var txtObj = document.getElementById('SearchFreeText');
   if(txtObj.value == sSearchFreeText)
    txtObj.value = "";
    
}
function SetFreeText()
{  
   var txtObj = document.getElementById('SearchFreeText');
   if(txtObj.value == "" )
   {
        var sSearchFreeText = "חפש";
        txtObj.value = sSearchFreeText;
    }
}

function CallSearch()
{
    var txtObj = document.getElementById('SearchFreeText');    
    var freeText = txtObj.value;
   var sSearchFreeText = "חפש";

   //Check search text not empty       
    if (trim(freeText) == "" || freeText == sSearchFreeText) {
        var strError = "יש להזין טקסט לחיפוש";
        alert(strError);
        document.getElementById('SearchFreeText').focus();
        return false;
    } 
   
    var objRegExp  =/^[a-zA-Z0-9_א-ת?!@. ,()]*$/;

    //Check search text is valid
    if (!objRegExp.test(freeText))
    {
        var strError = "טקסט שהוזן לא חוקי";
	    alert(strError);
        document.getElementById('SearchFreeText').focus();
        return false;
    }
    __doPostBack('DoSearchEvent', freeText);

    
}

 function trim(stringToTrim) {
        return stringToTrim.replace(/^\s+|\s+$/g, "");
    }

function usersearch(sUserSearch)
 { 
 
  if(typeof(sUserSearch) != "undefined")
    document.getElementById('SearchFreeText').value = sUserSearch;  
  }   
</script>
<table class="searchBoxGroup">
<tr>
<td>
     <input
            id="SearchFreeText" name="SearchFreeText"
            onfocus="ClearText();"
            onblur="SetFreeText();"
            onkeydown="if(event.keyCode==13) return CallSearch();"
             value="חפש" class="SearchFreeText" validation="/^[a-zA-Z0-9_א-ת?!@. ,()]*$/" maxlength="25"            
            />
</td>
<td>

<input type="button"
             class="searchBtnImg" onmouseover="this.className='searchBtnImgOver';" onmouseout="this.className='searchBtnImg';" value="" title="חפש"
            onclick="return CallSearch();"
            onkeypress="return CallSearch();"/>
            
</td>
</tr>
</table>
 
					
	</td>
	</tr>
	</table>
		
